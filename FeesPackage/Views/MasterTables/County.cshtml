@using FeesPackage.Data_Access;
@model List<tblCounty>

@{
    ViewBag.Title = "tblCounty";
}

<div class="span9">
    <div class="content">
        <div class="module">
            <div class="module-head">
                <h3>
                    <i>@ViewBag.Title</i> Table
                </h3>
            </div>
            <div class="module-body table">
                <table cellpadding="0" cellspacing="0" border="0" class="datatable-1 table table-bordered table-striped	display" width="100%">
                    <thead>
                        <tr>
                            <th>@Html.DisplayNameFor(modelItem => Model[0].County)</th>
                            <th>@Html.DisplayNameFor(modelItem => Model[0].County_Desc)</th>
                            <th>@Html.DisplayNameFor(modelItem => Model[0].County_Value)</th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (tblCounty model in Model)
                        {
                        <tr>
                            <td>@Html.DisplayFor(modelItem => model.County)</td>
                            <td>@Html.DisplayFor(modelItem => model.County_Desc)</td>
                            <td>@Html.DisplayFor(modelItem => model.County_Value)</td>
                        </tr>
                        }
                    </tbody>
                    <tfoot>
                        <tr>
                            <th>@Html.DisplayNameFor(modelItem => Model[0].County)</th>
                            <th>@Html.DisplayNameFor(modelItem => Model[0].County_Desc)</th>
                            <th>@Html.DisplayNameFor(modelItem => Model[0].County_Value)</th>
                        </tr>
                    </tfoot>
                </table>
            </div>
        </div>
        <!--/.module-->
    </div>
    <!--/.content-->
</div>
<!--/.span9-->
